    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "fast-glob": "^3.2.7",
    "http-errors": "^1.8.0",
    "morgan": "~1.9.1",
    "per-env": "^1.0.2",
    "pg": "^8.7.1",
