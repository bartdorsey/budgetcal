    "db:drop": "per-env",
    "db:drop:development": "dotenv sequelize db:drop",
    "db:seed:all": "per-env",
    "db:seed:all:development": "dotenv sequelize db:seed:all",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
