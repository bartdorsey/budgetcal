{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "per-env",
    "start:production": "node ./dist/bin/www.js",
    "start:development": "tsc-watch --onSuccess 'node -r dotenv/config ./dist/bin/www.js'",
    "db:migrate": "per-env",
    "db:migrate:development": "dotenv sequelize db:migrate",
    "db:create": "per-env",
    "db:create:development": "dotenv sequelize db:create",
    "db:drop": "per-env",
    "db:drop:development": "dotenv sequelize db:drop",
    "db:seed:all": "per-env",
    "db:seed:all:development": "dotenv sequelize db:seed:all"
  },
  "dependencies": {
    "@types/http-errors": "^1.8.1",
    "bcrypt": "^5.0.1",
    "connect-session-sequelize": "^7.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "http-errors": "^1.8.0",
    "morgan": "~1.9.1",
    "per-env": "^1.0.2",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",
    "sequelize-pretty-logger": "^1.0.2",
    "sequelize-typescript": "^2.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/common-errors": "^1.0.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.4.7",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-import": "^2.24.0",
    "ts-node": "^10.1.0",
    "tsc-watch": "^4.4.0",
    "typescript": "^4.3.5"
  }
}
